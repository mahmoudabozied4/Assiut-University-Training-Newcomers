#include<bits/stdc++.h>
using namespace std;
int main()
{
    while(true)
    {


    long long sum = 0 ;
    int n ,m ;cin >> n >> m ;
    if(n<=0 || m<=0)
        {
            return 0 ;
        }
        else
        {
            if(n>m)
                swap(n,m);
                for(int i = n ; i<=m ; i++)
            {
                cout << i << " " ;
                sum+=i;
            }
            cout <<"sum =" << sum << endl;

        }
}

    return 0 ;
}
